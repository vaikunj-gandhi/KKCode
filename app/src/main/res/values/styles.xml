        <item name="android:textColor">@color/text_primary</item>
        <item name="android:windowDisablePreview">true</item>
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
        <item name="android:actionMenuTextColor">@color/colorSurface</item>
        <item name="android:actionMenuTextAppearance">@style/MenuText</item>
        <item name="android:overScrollMode">never</item>
        <item name="sliderStyle">@style/Widget.App.Slider</item>

        <item name="bottomSheetDialogTheme">@style/AppBottomSheetDialogTheme</item>
        <item name="android:timePickerStyle">@style/MyTimePickerDialogStyle</item>
        <item name="android:datePickerStyle">@style/MyDatePickerDialogStyle</item>
        <item name="android:buttonBarNegativeButtonStyle">@style/TimePicker.ButtonBarNegativeButtonStyle</item>
        <item name="android:buttonBarPositiveButtonStyle">@style/TimePicker.ButtonBarNegativeButtonStyle</item>
        <item name="android:datePickerDialogTheme">@style/MyDatePickerDialogTheme</item>
    </style>

    <style name="Widget.Fusion.Toolbar" parent="Widget.AppCompat.Toolbar">
        <item name="android:background">@color/colorPrimary</item>

        <item name="popupTheme">@style/ThemeOverlay.AppCompat.Light</item>
        <item name="titleTextAppearance">@style/TextAppearance.Fusion.Toolbar</item>
        <item name="android:contentInsetStart">0dp</item>
        <item name="android:contentInsetLeft">0dp</item>
        <item name="android:contentInsetEnd">0dp</item>
        <item name="android:contentInsetRight">0dp</item>
        <item name="android:paddingLeft">0dp</item>
    </style>
    <style name="TextAppearance.Fusion.Toolbar" parent="TextAppearance.MaterialComponents.Button">
        <item name="android:textSize">16sp</item>
    </style>

    <style name="Font.SubTitle" parent="TextAppearance.AppCompat">
